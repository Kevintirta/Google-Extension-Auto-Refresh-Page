{
  "name": "auto_refresh_google_ext",
  "description": "A Vue.js web extension to refresh web pages in specified time interval",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_refresh_64.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "auto_refresh_google_ext",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
